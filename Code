#include <stdio.h>

int main() {
    // Write C code here
    int amount_given;
    int bill_amount;
    int q,r;
    scanf("%d%d",&amount_given,&bill_amount);
    q=amount_given/bill_amount;
    r=amount_given%bill_amount;
    printf("Quotient=%d",q);
    printf("\nRemainder=%d", r);
    return 0;
}
